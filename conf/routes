GET     /                           controllers.HomeController.index

GET 	/api/host/:name				controllers.HostController.getHost(name: String)
POST	/api/host					controllers.HostController.createHost
PUT		/api/host/:name				controllers.HostController.updateHost(name: String)
DELETE	/api/host/:name				controllers.HostController.deleteHost(name: String)
GET		/api/hosts					controllers.HostController.getAllHosts

POST	/api/client					controllers.ClientController.create
GET	/api/client/:name			controllers.ClientController.get(name: String)

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
